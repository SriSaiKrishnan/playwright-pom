<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Demo QA Regression PW Suite"  thread-count="2" parallel="tests" verbose="4">

    <listeners>
        <listener
                class-name="com.qa.demo.listeners.ExtentReportListener" />

    </listeners>

    <test name="Demo QA Textbox Page Test">
        <parameter name="browser" value="chrome" />
        <classes>
            <class name="com.qa.demo.tests.TextboxTest" />
        </classes>
    </test>

    <test name="Demo QA Browser Page Test">
        <parameter name="browser" value="chromium" />
        <classes>
            <class name="com.qa.demo.tests.BrowserTest" />
        </classes>
    </test>

</suite>